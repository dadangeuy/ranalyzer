<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefWidth="512.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ranalyzer.view.ModifyStatementView">
   <children>
      <VBox prefWidth="100.0">
         <children>
            <Label text="Statement" />
            <TextField fx:id="statement" />
         </children>
      </VBox>
      <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onClickSave" text="Save" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
